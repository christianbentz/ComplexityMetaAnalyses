geom_vline(data = med, aes(xintercept = grp.median),
linetype = "dashed", color = "red") +
geom_vline(aes(xintercept = 0), linetype = "dotted") +
labs(x = "Complexity Value", y = "Density") +
xlim(-3, 3) +
theme_bw()
